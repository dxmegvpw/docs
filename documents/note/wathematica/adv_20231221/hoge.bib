@article{Almheiri_EntropyBulk_2019,
  title = {The Entropy of Bulk Quantum Fields and the Entanglement Wedge of an Evaporating Black Hole},
  author = {Almheiri, Ahmed and Engelhardt, Netta and Marolf, Donald and Maxfield, Henry},
  year = {2019},
  month = dec,
  journal = {Journal of High Energy Physics},
  volume = {2019},
  number = {12},
  eprint = {1905.08762},
  primaryclass = {gr-qc, physics:hep-th},
  pages = {63},
  issn = {1029-8479},
  doi = {10.1007/JHEP12(2019)063},
  url = {http://arxiv.org/abs/1905.08762},
  urldate = {2023-11-17},
  abstract = {Bulk quantum fields are often said to contribute to the generalized entropy \${\textbackslash}frac\{A\}\{4G\_N\} +S\_\{{\textbackslash}mathrm\{bulk\}\}\$ only at \$O(1)\$. Nonetheless, in the context of evaporating black holes, \$O(1/G\_N)\$ gradients in \$S\_\{{\textbackslash}mathrm\{bulk\}\}\$ can arise due to large boosts, introducing a quantum extremal surface far from any classical extremal surface. We examine the effect of such bulk quantum effects on quantum extremal surfaces (QESs) and the resulting entanglement wedge in a simple two-boundary \$2d\$ bulk system defined by Jackiw-Teitelboim gravity coupled to a 1+1 CFT. Turning on a coupling between one boundary and a further external auxiliary system which functions as a heat sink allows a two-sided otherwise-eternal black hole to evaporate on one side. We find the generalized entropy of the QES to behave as expected from general considerations of unitarity, and in particular that ingoing information disappears from the entanglement wedge after a scambling time \${\textbackslash}frac\{{\textbackslash}beta\}\{2{\textbackslash}pi\} {\textbackslash}ln {\textbackslash}Delta S + O(1)\$ in accord with expectations for holographic implementations of the Hayden-Preskill protocol. We also find an interesting QES phase transition at what one might call the Page time for our process.},
  archiveprefix = {arxiv},
  keywords = {AdS/CFT,Black hole}
}

