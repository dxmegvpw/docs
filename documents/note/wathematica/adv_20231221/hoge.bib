@article{Calabrese_EntanglementEntropy_2009,
  title = {Entanglement Entropy and Conformal Field Theory},
  author = {Calabrese, Pasquale and Cardy, John},
  year = {2009},
  month = dec,
  journal = {Journal of Physics A: Mathematical and Theoretical},
  volume = {42},
  number = {50},
  eprint = {0905.4013},
  primaryclass = {cond-mat, physics:hep-th, physics:quant-ph},
  pages = {504005},
  issn = {1751-8113, 1751-8121},
  doi = {10.1088/1751-8113/42/50/504005},
  url = {http://arxiv.org/abs/0905.4013},
  urldate = {2023-11-09},
  abstract = {We review the conformal field theory approach to entanglement entropy. We show how to apply these methods to the calculation of the entanglement entropy of a single interval, and the generalization to different situations such as finite size, systems with boundaries, and the case of several disjoint intervals. We discuss the behaviour away from the critical point and the spectrum of the reduced density matrix. Quantum quenches, as paradigms of non-equilibrium situations, are also considered.},
  archiveprefix = {arxiv},
  keywords = {AdS/CFT}
}

@article{Headrick_EntanglementRenyi_2010,
  title = {Entanglement {{Renyi}} Entropies in Holographic Theories},
  author = {Headrick, Matthew},
  year = {2010},
  month = dec,
  journal = {Physical Review D},
  volume = {82},
  number = {12},
  eprint = {1006.0047},
  primaryclass = {cond-mat, physics:hep-th, physics:quant-ph},
  pages = {126010},
  issn = {1550-7998, 1550-2368},
  doi = {10.1103/PhysRevD.82.126010},
  url = {http://arxiv.org/abs/1006.0047},
  urldate = {2023-11-09},
  abstract = {Ryu and Takayanagi conjectured a formula for the entanglement (von Neumann) entropy of an arbitrary spatial region in an arbitrary holographic field theory. The von Neumann entropy is a special case of a more general class of entropies called Renyi entropies. Using Euclidean gravity, Fursaev computed the entanglement Renyi entropies (EREs) of an arbitrary spatial region in an arbitrary holographic field theory, and thereby derived the RT formula. We point out, however, that his EREs are incorrect, since his putative saddle points do not in fact solve the Einstein equation. We remedy this situation in the case of two-dimensional CFTs, considering regions consisting of one or two intervals. For a single interval, the EREs are known for a general CFT; we reproduce them using gravity. For two intervals, the RT formula predicts a phase transition in the entanglement entropy as a function of their separation, and that the mutual information between the intervals vanishes for separations larger than the phase transition point. By computing EREs using gravity and CFT techniques, we find evidence supporting both predictions. We also find evidence that large-\$N\$ symmetric-product theories have the same EREs as holographic ones.},
  archiveprefix = {arxiv},
  keywords = {AdS/CFT}
}

@article{Headrick_LecturesEntanglement_2019,
  title = {Lectures on Entanglement Entropy in Field Theory and Holography},
  author = {Headrick, Matthew},
  year = {2019},
  month = jul,
  number = {arXiv:1907.08126},
  eprint = {1907.08126},
  primaryclass = {cond-mat, physics:gr-qc, physics:hep-th},
  publisher = {{arXiv}},
  url = {http://arxiv.org/abs/1907.08126},
  urldate = {2023-08-29},
  abstract = {These notes, based on lectures given at various schools over the last few years, aim to provide an introduction to entanglement entropies in quantum field theories, including holographic ones. We explore basic properties and simple examples of entanglement entropies, mostly in two dimensions, with an emphasis on physical rather than formal aspects of the subject. In the holographic case, the focus is on how the Ryu-Takayanagi formula geometrically realizes general features of field-theory entanglement, while revealing special properties of holographic theories. In order to make the notes somewhat self-contained for readers whose background is in high-energy theory, a brief introduction to the relevant aspects of quantum information theory is included.},
  archiveprefix = {arxiv},
  keywords = {AdS/CFT,Lecture Note}
}

